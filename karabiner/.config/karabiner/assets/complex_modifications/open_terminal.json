{
  "title": "Open Terminal with Cmd+Enter",
  "rules": [
    {
      "description": "Open Terminal with Command+Enter",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "shell_command": "open -a alacritty"
            }
          ]
        }
      ]
    }
  ]
}

