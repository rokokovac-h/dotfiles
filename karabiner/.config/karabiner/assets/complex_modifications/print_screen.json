{
  "title": "Cmd+Shift+X -> PrintScreen",
  "rules": [
    {
      "description": "Bind Cmd + Shift + X to Print Screen",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "x",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_shift"
              ],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "print_screen"
            }
          ]
        }
      ]
    }
  ]
}
